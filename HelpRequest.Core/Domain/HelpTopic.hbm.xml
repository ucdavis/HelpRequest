<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="HelpRequest.Core" namespace="HelpRequest.Core.Domain">
  <class
    name="HelpTopic"
    table="HelpTopics" >
    <id name="Id" column="id">
      <generator class="identity" />
    </id>

    <property name="Question" />
    <property name="Answer" />

    <property name="IsActive"/>
    <property name="AvailableToPublic" />
    <property name="AppFilter" />
      
    <property name="NumberOfReads"/>
    <property name="IsVideo"/>
    <property name="VideoName" />

  </class>
</hibernate-mapping>